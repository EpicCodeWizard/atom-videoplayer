'.videoplayer':
    'space':        'videoplayer:toggle-playback'
    'enter':        'videoplayer:toggle-playback'
    'left':         'videoplayer:seek-backward'
    'right':        'videoplayer:seek-forward'
    'up':           'videoplayer:goto-start'
    'down':         'videoplayer:goto-end'
    'm':            'videoplayer:toggle-mute'
